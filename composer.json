{
    "name": "spatie/laravel-analytics-tracker",
    "description": "Keeps track of the original UTM parameters",
    "keywords": [
        "spatie",
        "laravel-utm-forwarder"
    ],
    "homepage": "https://github.com/spatie/laravel-utm-forwarder",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex.vanderbist@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\AnalyticsTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\AnalyticsTracker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "AnalyticsTrackerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
